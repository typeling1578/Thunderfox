{
  "manifest_version": 2,
  "name": "System Addon",
  "version": "1.3",
  "applications": {
    "gecko": {
      "id": "system-addon@typeling1578",
      "strict_min_version": "102.0"
    }
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "storage",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "background.js",
      "ua_override.js",
      "ua_override_sites.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://www.youtube.com/*",
        "*://music.youtube.com/*"
      ],
      "js": ["inject/youtube.js"],
      "run_at": "document_start"
    }
  ],
  "experiment_apis": {
    "aboutConfigPrefs": {
      "schema": "experimental-apis/aboutConfigPrefs.json",
      "parent": {
        "scopes": [
          "addon_parent"
        ],
        "script": "experimental-apis/aboutConfigPrefs.js",
        "paths": [
          [
            "aboutConfigPrefs"
          ]
        ]
      }
    },
    "browserInfo": {
      "schema": "experimental-apis/browserInfo.json",
      "parent": {
        "scopes": [
          "addon_parent"
        ],
        "script": "experimental-apis/browserInfo.js",
        "paths": [
          [
            "browserInfo"
          ]
        ]
      }
    },
    "browserLangSettings": {
      "schema": "experimental-apis/browserLangSettings.json",
      "parent": {
        "scopes": [
          "addon_parent"
        ],
        "script": "experimental-apis/browserLangSettings.js",
        "paths": [
          [
            "browserLangSettings"
          ]
        ]
      }
    }
  }
}